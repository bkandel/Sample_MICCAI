	Visual, physiology; Female; Fourier Analysis; Humans; Image Enhancement;
	Magnetic Resonance Imaging, statistics /&/ numerical data; Male;
	Mathematical Computing; Oxygen Consumption, physiology; Oxygen, blood;
	Pattern Recognition, Visual, physiology; Regional Blood Flow, physiology;
	Research Design; Sensitivity and Specificity; Visual Cortex, blood
	supply/physiology},
  language = {eng},
  medline-pst = {ppublish},
  owner = {jtduda},
  pii = {S1053811901909905},
  pmid = {11848692},
  timestamp = {2013.02.25},
  url = {http://dx.doi.org/10.1006/nimg.2001.0990}
}

